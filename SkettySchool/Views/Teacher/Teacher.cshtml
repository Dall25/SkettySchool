@model List<SkettySchool.Models.TeacherModel>

@{
    ViewData["Title"] = "Teacher";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Teacher</h1>
<div><b>Teacher Information</b><br /></div>  
<table class="table table-bordered table-responsive table-hover">  
    <tr>  
        <th>Teacher Id </th>  
        <th>First Name </th>  
        <th>Last Name </th>  
        <th>User Type</th>    
    </tr>  
    @foreach (var t in Model)  
    {  
        <tr>  
            <td>@t.TeacherId</td>  
            <td>@t.FirstName</td>  
            <td>@t.LastName</td>  
            <td>@t.UserType</td>    
  
        </tr>  
    }  
</table>  

<div>
    <a asp-action="Index">Back to List</a>
</div>